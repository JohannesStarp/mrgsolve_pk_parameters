$PROBLEM LZD_Toepper_2016
$INPUT
ID OID SITE TIME TAD EVID DV BLQ MDV0=DROP MDVAP=MDV MDV1=DROP MDV2=DROP AMT CMT RATE DUR DI ROUTE OCC COUNTDV LASTDV 
SCR RRT QUICK BILI AST ALT ALBU LDH THRO MAP LAC ARDS HB PERITONITIS FIB
APACHE WBC SEPSIS CRP ECMO AGE SEX HT TBW

$DATA;
data_all.csv IGNORE=@;
IGNORE(ROUTE.EQ.3); only IV Data

$SUBROUTINES ADVAN13 TOL=6

$MODEL
COMP=(ORAL)
COMP=(CENT)
COMP=(AUC)

$PK
CL  = THETA(1) * EXP(ETA(1))
V2  = THETA(2) * EXP(ETA(2))

$DES
DADT(1)= 0
DADT(2)= -A(2)*(CL/V2)
DADT(3)= A(2)/V2

$THETA
(5.71)  ; 	1 CL
(41.1)  ; 	2 V

$OMEGA
(0.775) ; 1 IIV CL
(0.253) ; 2 IIV V1

$SIGMA

0.347 ; 1 additive
0.027 ; 2 proportional
 
$ERROR
IPRED = A(2)/V2
    W = SQRT(SIGMA(2)*IPRED**2 + SIGMA(1))
    Y = IPRED + EPS(1) + IPRED*EPS(2)
 IRES = DV-IPRED
IWRES = IRES/W
AUC= A(3)

$ESTIMATION METHOD=1 INTERACTION MAXEVAL=0 SIG=3 PRINT=1 NOABORT POSTHOC
$TABLE ID TIME TAD EVID MDV OCC DV PRED IPRED SITE RRT SEPSIS SEX LASTDV COUNTDV AUC ONEHEADER NOAPPEND NOPRINT FILE=sdtab124

